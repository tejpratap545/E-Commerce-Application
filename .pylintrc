[MASTER]
load-plugins=pylint_django , pylint_celery
disable=
    C0114,C0115,C0116 # missing-module-docstring

[FORMAT]
max-line-length=120

[MESSAGES CONTROL]
disable=bad-continuation,wrong-import-order

[DESIGN]
max-parents=13

[TYPECHECK]
generated-members=REQUEST,acl_users,aq_parent,"[a-zA-Z]+_set{1,2}",save,delete

